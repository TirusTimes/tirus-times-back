// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://tirusTimes:1234@localhost:5432/prisma-postgres?schema=public"
}

model User {
  id         Int      @id @default(autoincrement())
  username   String   @unique
  firstname  String
  lastname   String
  email      String   @unique
  password   String
  position   String
  age        Int
  gender     String
  created_at DateTime @default(now())
}
